{
  "name": "nextjs-personal-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint": "next lint",
    "prepare": "husky install",
    "lint-staged": "next lint",
    "format": "eslint --fix './{lib,components,pages}/**/*.{ts,tsx}'",
    "start": "yarn run relay && next start",
    "build": "yarn run relay && next build",
    "relay": "yarn run relay-compiler $@",
    "bundle-analyzer": "ANALYZE=true next build"
  },
  "relay": {
    "src": "./",
    "schema": "./schema/schema.graphql",
    "language": "typescript",
    "artifactDirectory": "queries/__generated__"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@types/nodemailer": "^6.4.4",
    "antd": "^4.16.13",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "gray-matter": "^4.0.3",
    "next": "12.1.6",
    "next-seo": "^4.28.1",
    "nodemailer": "^6.7.0",
    "react": "^18.0.0-rc.3",
    "react-dom": "^18.0.0-rc.3",
    "react-html-parser": "^2.0.2",
    "react-markdown": "^7.1.0",
    "react-relay": "^13.2.0",
    "react-syntax-highlighter": "^15.4.5",
    "relay-runtime": "^13.2.0"
  },
  "devDependencies": {
    "@types/react": "17.0.24",
    "@types/react-relay": "^13.0.1",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/relay-runtime": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-plugin-relay": "^13.2.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "graphql": "^16.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "relay-compiler": "^13.2.0",
    "typescript": "4.4.3"
  }
}
